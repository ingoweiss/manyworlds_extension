{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Manyworlds",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.manyworlds",
          "match": "\\b(Given|When|Then|And|But)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.manyworlds",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.manyworlds",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.manyworlds"
}