{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Manyworlds",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#scenario_element"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.manyworlds",
          "match": "\\b(Given|When|Then|And|But)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.manyworlds",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.manyworlds",
          "match": "\\\\."
        }
      ]
    },
    "scenario_element": {
      "captures": {
        "1": {
          "name": "keyword.language.gherkin.feature.scenario"
        },
        "2": {
          "name": "string.language.gherkin.scenario.title.title"
        }
      },
      "match": "^\\s*(Scenario:)([^#]*)(\\s+#.*)?$"
  }
  },
  "scopeName": "source.manyworlds"
}